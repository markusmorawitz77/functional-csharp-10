    "Formats": [
      {
        "Name": "Inline",
        "HorizontalMargin": 5,
        "VerticalMargin": 3,
        "BarHeight": 25,
        "ThinBarWidth": 1.5,
        "ThickBarWidth": 4,
        "GapWidth": 2,
        "Padding":2,
        "Antialias": true
      },
      {
        "Name": "Print",
        "HorizontalMargin": 20,
        "VerticalMargin": 10,
        "BarHeight": 200,
        "ThinBarWidth": 4,
        "ThickBarWidth": 10,
        "GapWidth": 4,
        "Padding": 4,
        "Antialias": false
      }
    ]
  }
}
